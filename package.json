  "repository": {
    "type": "git",
    "url": "https://github.com/larcjs/core-types.git"
  },
  "bugs": {
    "url": "https://github.com/larcjs/core-types/issues"
  },
  "homepage": "https://github.com/larcjs/core-types#readme"
}
