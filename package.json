{
  "name": "@larcjs/core-types",
  "version": "1.0.0",
  "description": "TypeScript type definitions for @larcjs/core",
  "types": "index.d.ts",
  "type": "module",
  "files": [
    "**/*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts"
    },
    "./types/message": {
      "types": "./types/message.d.ts"
    },
    "./types/subscription": {
      "types": "./types/subscription.d.ts"
    },
    "./types/config": {
      "types": "./types/config.d.ts"
    },
    "./components/pan-client": {
      "types": "./components/pan-client.d.ts"
    },
    "./components/pan-bus": {
      "types": "./components/pan-bus.d.ts"
    },
    "./components/pan-autoload": {
      "types": "./components/pan-autoload.d.ts"
    }
  },
  "peerDependencies": {
    "@larcjs/core": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@larcjs/core": {
      "optional": true
    }
  },
  "keywords": [
    "typescript",
    "types",
    "larc",
    "pan",
    "web-components",
    "message-bus",
    "pubsub",
    "zero-build"
  ],
